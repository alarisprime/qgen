{
  "name": "qgen",
  "version": "0.0.0-development",
  "description": "Generate files from templates",
  "bin": {
    "qgen": "./bin/cli.js"
  },
  "scripts": {
    "watch:test": "npm run unit-test -- -w npm run unit-test -- -w --source './test/fixtures/*/src/**/*' './test/fixutres/*/expected/**/*' './test/*.js'",
    "unit-test": "ava",
    "lint": "xo",
    "test": "npm run lint && npm run unit-test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": "alarisprime/qgen",
  "keywords": [
    "cli",
    "generator",
    "templates",
    "files"
  ],
  "author": "Saneef Ansari <hello@saneef.com> (https://saneef.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alarisprime/qgen/issues"
  },
  "homepage": "https://github.com/alarisprime/qgen#readme",
  "dependencies": {
    "chalk": "^2.0.0",
    "globby": "^6.0.0",
    "handlebars": "^4.0.5",
    "inquirer": "^3.0.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "nested-error-stacks": "^2.0.0",
    "pinkie-promise": "^2.0.1",
    "redent": "^2.0.0",
    "trim-newlines": "^2.0.0"
  },
  "devDependencies": {
    "assert-dir-equal": "^1.0.1",
    "ava": "^0.20.0",
    "cpy": "^5.0.0",
    "del": "^3.0.0",
    "execa": "^0.7.0",
    "semantic-release": "^6.3.6",
    "xo": "^0.19.0"
  },
  "xo": {
    "esnext": true
  },
  "ava": {
    "failWithoutAssertions": false
  }
}
